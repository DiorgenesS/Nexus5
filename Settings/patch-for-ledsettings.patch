From 79a0083587f0d515a45b238bc427631db88bf07c Mon Sep 17 00:00:00 2001
From: Diorgenes <dyogenes_gd@hotmail.com>
Date: Wed, 3 May 2017 01:16:03 -0300
Subject: [PATCH] Enable only led for notifications settings

---
 Settings/res/xml/led_settings.xml                     | 1 -
 Settings/smali/com/android/settings/LedSettings.smali | 4 ++--
 2 files changed, 2 insertions(+), 3 deletions(-)


--- a/Settings/res/xml/led_settings.xml
+++ b/Settings/res/xml/led_settings.xml
@@ -2,7 +2,6 @@
 <PreferenceScreen android:title="@string/trackball_settings"
   xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
-    <CheckBoxPreference android:persistent="false" android:title="@string/battery_light_title" android:key="battery_light" android:summary="@string/battery_light_summary" />
     <CheckBoxPreference android:persistent="false" android:title="@string/notification_pulse_title" android:key="notification_pulse" android:summary="@string/led_pulse_summary" />
     <ListPreference android:persistent="false" android:entries="@array/led_color_entries" android:title="@string/notification_breathing_light_color_title" android:key="breathing_light_color" android:dependency="notification_pulse" android:dialogTitle="@string/breathing_light_color_menu_title" android:entryValues="@array/led_color_values" />
     <ListPreference android:persistent="false" android:entries="@array/led_freq_entries" android:title="@string/notification_breathing_light_freq_title" android:key="breathing_light_freq" android:dependency="notification_pulse" android:dialogTitle="@string/breathing_light_freq_title" android:entryValues="@array/led_freq_values" />

--- a/Settings/smali/com/android/settings/LedSettings.smali
+++ b/Settings/smali/com/android/settings/LedSettings.smali
@@ -611,7 +611,7 @@
 
     iget-object v0, p0, Lcom/android/settings/LedSettings;->pp:Landroid/preference/CheckBoxPreference;
 
-    invoke-virtual {v0, p0}, Landroid/preference/CheckBoxPreference;->setOnPreferenceChangeListener(Landroid/preference/Preference$OnPreferenceChangeListener;)V
+    #invoke-virtual {v0, p0}, Landroid/preference/CheckBoxPreference;->setOnPreferenceChangeListener(Landroid/preference/Preference$OnPreferenceChangeListener;)V
 
     iget-object v0, p0, Lcom/android/settings/LedSettings;->pp:Landroid/preference/CheckBoxPreference;
 
@@ -629,7 +629,7 @@
     if-ne v3, v1, :cond_7
 
     :goto_1
-    invoke-virtual {v0, v1}, Landroid/preference/CheckBoxPreference;->setChecked(Z)V
+    #invoke-virtual {v0, v1}, Landroid/preference/CheckBoxPreference;->setChecked(Z)V
 
     return-void
 
-- 
2.7.4

